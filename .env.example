# Database
DATABASE_URL=postgres://user:password@localhost:5432/janusz

# Redis
REDIS_URL=redis://localhost:6379

# GitHub App Credentials
# Create at: https://github.com/settings/apps
GITHUB_APP_ID=your_app_id
GITHUB_PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY-----\nyour_private_key_here\n-----END RSA PRIVATE KEY-----"
GITHUB_CLIENT_ID=your_client_id
GITHUB_CLIENT_SECRET=your_client_secret

# Webhook Secret
# Set in GitHub App settings
WEBHOOK_SECRET=your_webhook_secret

# Google Gemini API
# Get key at: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key

# OpenRouter API
# Get key at: https://openrouter.ai/settings/keys
OPENROUTER_API_KEY=your_openrouter_api_key

# Queue Configuration (optional)
CONCURRENCY=5
QUEUE_NAME=pr-review

# Session Secret (for nuxt-auth-utils)
NUXT_SESSION_PASSWORD=at_least_32_characters_long_random_string
